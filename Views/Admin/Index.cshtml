
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link rel="stylesheet" href="~/Content/AdminDangNhap.css" />
    <title>Admin</title>
</head>
<body>
    <div class="main">
        <div class="main__box">
            <p class="main__box--title">
                <span>ADMIN WEB</span> 
            </p>
            <form class="main__box--form">
                <div class="form-group">
                    
                    <input type="email" class="form-control" placeholder="Tài khoản">
                   
                </div>
                <div class="form-group">
                    
                    <input type="password" class="form-control" placeholder="Mật khẩu">
                </div>
                <div class="main__box--click">
                    <button type="submit" class="btn">ĐĂNG NHẬP</button>

                </div>
            </form>

        </div>

    </div>
</body>
</html>
